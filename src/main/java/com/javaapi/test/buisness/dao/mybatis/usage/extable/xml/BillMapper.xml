<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.javaapi.test.buisness.dao.mybatis.usage.extable.mapper.BillMapper">
    <select id="selectByBill" parameterType="java.lang.Long"
            resultType="com.javaapi.test.buisness.dao.mybatis.usage.extable.entity.Bill">
        select *
        from bill_detail
        where bill_id = #{billId}
    </select>
</mapper> 